<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="escape.Alumno" table="alumno" >
        <id column="idAlumno" name="idAlumno" />
        <property column="apellidos" name="apellidos" />
        <property column="edad" name="edad" />
        <property column="nombre" name="nombre" />
        <property column="repetidor" name="repetidor" />
    </class>
</hibernate-mapping>
