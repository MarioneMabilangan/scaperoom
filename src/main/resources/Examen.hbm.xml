<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="escape.Examen" table="examen" >
        <id column="idExamen" name="idExamen" />
        <property column="fecha" name="fecha" />
        <property column="nota" name="nota" />
        <property column="idAlumno" name="idAlumno" />
        <property column="idModul" name="idModul" />
    </class>
</hibernate-mapping>
